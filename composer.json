    "name": "mkabischev/yii-extension-installer",
    "type": "composer-installer",
    "autoload": {
        "psr-0": {
            "Composer": "src/"
        }
    },
    "require": {
        "composer/installers": "1.0.*@dev"
    },
    "extra": {
        "class": "Composer\\Installer\\YiiExtensionInstaller"
    },

    "require-dev": {
        "composer/composer": "*@dev"
    }
}